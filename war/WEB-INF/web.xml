<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

<servlet-mapping>
	<servlet-name>service</servlet-name>
	<url-pattern>/service</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>service</servlet-name>
	<servlet-class>com.desklampstudios.edab.ServiceServlet3</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Login</servlet-name>
	<url-pattern>/login</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Login</servlet-name>
	<servlet-class>com.desklampstudios.edab.LoginServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>LoginCallback</servlet-name>
	<url-pattern>/logincallback</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>LoginCallback</servlet-name>
	<servlet-class>com.desklampstudios.edab.LoginCallbackServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>Logout</servlet-name>
	<url-pattern>/logout</url-pattern>
</servlet-mapping>
<servlet>
	<servlet-name>Logout</servlet-name>
	<servlet-class>com.desklampstudios.edab.LogoutServlet</servlet-class>
</servlet>

<!-- Objectify filter -->
<filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
</filter>
<filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
</filter-mapping>

<!--  Serve index.html as the root -->
<welcome-file-list>
	<welcome-file>index.html</welcome-file>
</welcome-file-list>

<!--  Always serve HTTPS. -->
<security-constraint>
    <web-resource-collection>
        <url-pattern>/*</url-pattern>
    </web-resource-collection>
    <user-data-constraint>
        <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
</security-constraint>

<session-config>
    <session-timeout>604800</session-timeout>
</session-config>

<!-- Serve STS for one week -->
<static-files>
    <include path="/*">
        <http-header name="Strict-Transport-Security" value="max-age=604800"/>
    </include>
</static-files>

